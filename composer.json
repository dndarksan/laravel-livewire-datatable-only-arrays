{
    "name": "dndarksan/laravel-livewire-datatable-only-arrays",
    "description": "A dynamic table component for Laravel Livewire",
    "keywords": [
        "dndarksan",
        "livewire",
        "datatables",
        "tables",
        "laravel"
    ],
    "homepage": "https://github.com/dndarksan/laravel-livewire-datatable-only-arrays",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Chavez",
            "email": "dndarksan@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "livewire/livewire": "^3.0|dev-main"
    },
    "require-dev": {
        "brianium/paratest": "^5.0|^6.0|^7.0|^8.0",
        "laravel/pint": "^1.10",
        "monolog/monolog": "*",
        "nunomaduro/collision": "^6.0|^7.0|^8.0",
        "nunomaduro/larastan": "^2.6",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Dndarksan\\LaravelLivewireDatatables\\": "src"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dndarksan\\LaravelLivewireDatatables\\LaravelLivewireDatatablesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}